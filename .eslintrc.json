{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "no-underscore-dangle": "off",
    "no-console": "off",
    "max-len": 0,
    "no-shadow": 0,
    "no-alert": 0,
    "no-restricted-globals": 0,
    "linebreak-style": 0,
    "consistent-return": 0,
    "no-prototype-builtins": 0,
    "import/prefer-default-export": 0,
    "no-return-assign": 0,
    "no-unused-vars": "warn",
    "import/no-unresolved": "off",
    "import/extensions": [
      "error",
      "always",
      {
        "js": "never",
        "jsx": "never"
      }
    ],
    "no-useless-concat": "error",
    "no-empty-function": [
      "error",
      {
        "allow": ["functions"]
      }
    ],
    "no-plusplus": [
      "error",
      {
        "allowForLoopAfterthoughts": true
      }
    ],
    "func-names": "off",
    "no-use-before-define": "off"
    // "no-use-before-define": ["error", { "functions": false, "classes": true, "variables": false }]
  }
}

